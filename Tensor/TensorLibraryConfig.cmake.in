@PACKAGE_INIT@

set(TENSOR_LIBRARY_DIR "${CMAKE_CURRENT_LIST_DIR}")

include(CMakeFindDependencyMacro)

# Dependencies can be added here
find_dependency(Threads)

# Include the TensorLibTargets file, which defines how to link the library
include("${TENSOR_LIBRARY_DIR}/TensorLibTargets.cmake")

# Declare the shared library as imported
add_library(Tensor::tensor_lib_shared SHARED IMPORTED)

# Set the location of the shared library
set_target_properties(Tensor::tensor_lib_shared PROPERTIES
    IMPORTED_LOCATION "${TENSOR_LIBRARY_DIR}/libtensor_lib_shared.so"
    INTERFACE_INCLUDE_DIRECTORIES "${TENSOR_LIBRARY_DIR}/../include"
)
